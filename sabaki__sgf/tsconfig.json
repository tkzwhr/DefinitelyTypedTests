{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "esModuleInterop": true,
    "baseUrl": "../",
    "paths": {
      "@sabaki/sgf": [
        "definitelytyped/types/sabaki__sgf"
      ]
    }
  },
  "include": ["src/main.cts", "tests/**/*.ts"]
}
