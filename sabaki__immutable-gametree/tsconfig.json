{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "esModuleInterop": true,
    "baseUrl": "../",
    "paths": {
      "@sabaki/immutable-gametree": [
        "definitelytyped/types/sabaki__immutable-gametree"
      ]
    }
  },
  "include": ["src/**/*.cts", "tests/**/*.ts"]
}
